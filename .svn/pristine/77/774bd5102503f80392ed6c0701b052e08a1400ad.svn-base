<view data-event-opts="{{[['tap',[['dowInfo',['$event']]]]]}}" class="previewWeb" bindtap="__e"><headertop vue-id="530a576e-1" titleTop="{{titleTop}}" bind:__l="__l"></headertop><view class="previewWeb_main" style="{{('min-height: calc(100vh - ('+(staHeight*2+80)+'rpx) - 190rpx);')}}"><view class="previewWeb_main_center"><block wx:if="{{obj.type==1&&(obj.accessory_type=='docx'||obj.accessory_type=='doc')}}"><image class="img" mode="widthFix" src="../../static/home/word.png"></image></block><block wx:if="{{obj.type==1&&(obj.accessory_type=='ppt'||obj.accessory_type=='pptx')}}"><image class="img" mode="widthFix" src="../../static/home/ppt.png"></image></block><block wx:if="{{obj.type==1&&obj.accessory_type=='pdf'}}"><image class="img" mode="widthFix" src="../../static/home/pdf.png"></image></block><block wx:if="{{obj.type==1&&(obj.accessory_type=='xls'||obj.accessory_type=='xlsx')}}"><image class="img" mode="widthFix" src="../../static/home/excel.png"></image></block><block wx:if="{{obj.type==1&&(obj.accessory_type=='rar'||obj.accessory_type=='zip')}}"><image class="img" mode="widthFix" src="../../static/home/zip.png"></image></block><block wx:if="{{obj.type==1&&(obj.accessory_type=='png'||obj.accessory_type=='jpg')}}"><image class="img" mode="widthFix" src="../../static/home/img.png"></image></block><block wx:if="{{obj.type==1&&obj.accessory_type=='txt'}}"><image class="img" mode="widthFix" src="../../static/home/txt.png"></image></block></view><view class="info"><view class="name">{{obj.accessory_name}}</view><view class="size">{{'文件大小：'+obj.accessory_size+'M'}}</view><block wx:if="{{againShow}}"><view class="load"><image class="load_icon" src="../../static/user/load_03.png"></image><view class="txt">正在加载</view></view></block><block wx:else><button data-event-opts="{{[['tap',[['getFilePath',['$event']]]]]}}" class="again" style="{{(zipShow?'display:none;':'display:block;')}}" bindtap="__e">{{againTxt}}</button></block><block wx:if="{{zipShow}}"><view class="zipShow">请前往PC端操作</view></block></view></view><view class="footer_but"><view data-event-opts="{{[['tap',[['showInfo',['$event']]]]]}}" class="information" catchtap="__e"><block wx:if="{{information}}"><image class="img" src="../../static/user/shuxin.png"></image></block><block wx:else><image class="img" src="../../static/user/shuxin_no.png"></image></block><view class="txt">信息</view><block wx:if="{{information}}"><view class="supernatant"><view class="name">{{obj.accessory_name}}</view><view class="size">{{'文件大小：'+obj.accessory_size+'M'}}</view><view class="time">{{"最近更新："+obj.tt}}</view><view class="upname">{{"更新人："+obj.nickname}}</view><view class="triangleDiv"></view></view></block></view><block wx:if="{{imgCode==''&&obj.scope==1}}"><button class="share" open-type="getUserInfo" withCredentials="true" data-event-opts="{{[['getuserinfo',[['upImgIcon',['$event']]]]]}}" bindgetuserinfo="__e"><image class="img" src="../../static/user/fenxiang.png" data-event-opts="{{[['tap',[['upImgIcon',['$event']]]]]}}" bindtap="__e"></image><view data-event-opts="{{[['tap',[['upImgIcon',['$event']]]]]}}" class="txt" bindtap="__e">分享问题</view></button></block><block wx:if="{{!imgCode==''&&obj.scope==1}}"><button class="share" open-type="share"><image class="img" src="../../static/user/fenxiang.png"></image><view class="txt">分享问题</view></button></block><block wx:if="{{obj.scope==2}}"><button data-event-opts="{{[['tap',[['shareNo',['$event']]]]]}}" class="share" bindtap="__e"><image class="img" src="../../static/user/fenxiang.png"></image><view class="txt">分享问题</view></button></block><view data-event-opts="{{[['tap',[['collect',['$0'],['obj.id']]]]]}}" class="collect" bindtap="__e"><block wx:if="{{obj.fav==1}}"><image class="img" src="../../static/home/star_true.png"></image></block><block wx:if="{{obj.fav==0}}"><image class="img" src="../../static/home/star_false.png"></image></block><view class="txt">收藏</view></view><view class="haibao"><canvas style="{{('width:'+218*rpx+'px;height:'+175*rpx+'px;')}}" id="code-canvas" canvas-id="codecanvas"></canvas></view></view></view>