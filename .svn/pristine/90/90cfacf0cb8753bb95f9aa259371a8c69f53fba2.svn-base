.shade_popup {
  position: absolute;
  top: calc(25px + 240rpx);
  right: 0;
  left: 0;
  bottom: 0;
  z-index: 100;
  background: rgba(0, 0, 0, 0.4);
  display: none;
}
.shade_popup .popup_content {
  background: #fff;
}
.shade_popup .popup_content .select_item_av {
  background: #efeff4;
}
.shade_popup .popup_content .select_item_av > .txt {
  color: rgba(0, 0, 0, 0.85);
}
.shade_popup .popup_content .select_item {
  height: 82rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  padding-left: 36rpx;
}
.shade_popup .popup_content .select_item > .img {
  width: 46rpx;
  height: 54rpx;
  display: block;
  margin-right: 23rpx;
}
.shade_popup .popup_content .select_item > .img1 {
  width: 29rpx;
  height: 35rpx;
  display: block;
  margin-right: 23rpx;
}
.shade_popup .popup_content .select_item > .txt {
  line-height: 51rpx;
  font-size: 26rpx;
  color: rgba(0, 0, 0, 0.45);
}
.show_popup {
  display: block !important;
}
