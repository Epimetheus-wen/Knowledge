<view class="previewWeb"><headertop vue-id="4258b8ba-1" titleTop="{{titleTop}}" bind:__l="__l"></headertop><view class="previewWeb_main" style="{{('min-height: calc(100vh - ('+(staHeight*2+80)+'rpx));')}}"><view class="previewWeb_main_top"><view class="top_title">{{obj.title}}</view><view class="top_item"><view class="top_item_bq">解决方案</view><view class="top_item_time">创建<text>{{obj.tt}}</text></view></view><block wx:if="{{loginType}}"><view data-event-opts="{{[['tap',[['toClassifyList',['$0'],['obj.name']]]]]}}" class="top_item_but" bindtap="__e"><view class="txt">进入<text>{{obj.name}}</text>阅读更多</view></view></block></view><view class="previewWeb_main_center"><rich-text class="center_main" nodes="{{obj.answer}}"></rich-text></view><view class="previewWeb_main_bottom"><view class="item"><view class="item_txt">最近修改：</view><view class="item_num">{{obj.tt}}</view></view><view class="item"><view class="item_txt">更新人：</view><view class="item_num">{{obj.nickname}}</view></view></view></view><block wx:if="{{loginType}}"><view class="footer_but"><block wx:if="{{imgCode==''&&obj.scope==1}}"><button class="share" open-type="getUserInfo"><image class="img" src="../../static/user/fenxiang.png"></image><view class="txt">分享问题</view></button></block><block wx:if="{{!imgCode==''&&obj.scope==1}}"><button class="share" open-type="share"><image class="img" src="../../static/user/fenxiang.png"></image><view class="txt">分享问题</view></button></block><block wx:if="{{obj.scope==2}}"><button data-event-opts="{{[['tap',[['shareNo',['$event']]]]]}}" class="share" bindtap="__e"><image class="img" src="../../static/user/fenxiang.png"></image><view class="txt">分享问题</view></button></block><view data-event-opts="{{[['tap',[['collect',['$0'],['obj.id']]]]]}}" class="collect" bindtap="__e"><block wx:if="{{obj.fav==1}}"><image class="img" src="../../static/home/star_true.png"></image></block><block wx:if="{{obj.fav==0}}"><image class="img" src="../../static/home/star_false.png"></image></block><view class="txt">收藏</view></view><view class="haibao"><canvas style="{{('width:'+218*rpx+'px;height:'+175*rpx+'px;')}}" id="code-canvas" canvas-id="codecanvas"></canvas></view></view></block></view>