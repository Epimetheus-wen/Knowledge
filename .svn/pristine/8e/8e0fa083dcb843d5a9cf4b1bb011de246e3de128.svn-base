                            // 显示区域长度
                            var slideYY=0
                            if(this.isRotating){
                                // 横屏
                                // slideYY=this.windowHeight-40
                            }else{
                                slideYY=((this.windowHeight-this.staHeight)/this.rpx)-326
                            }
                            this.slideYY=slideYY
