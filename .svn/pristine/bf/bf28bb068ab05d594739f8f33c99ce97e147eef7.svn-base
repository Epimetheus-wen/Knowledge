.previewWeb .previewWeb_main {
  min-height: calc(100vh - (25px + 80rpx) - 190rpx);
  background: #EFEFF4;
  padding-top: 190rpx;
}
.previewWeb .previewWeb_main .previewWeb_main_center {
  width: 360rpx;
  height: 280rpx;
  background: white;
  border-radius: 6rpx;
  border: 2rpx solid rgba(0, 0, 0, 0.1);
  margin: 0 auto 30rpx auto;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.previewWeb .previewWeb_main .previewWeb_main_center .img {
  width: 80rpx;
  height: 96rpx;
  display: block;
}
.previewWeb .previewWeb_main .info {
  margin: 0 auto;
  width: 680rpx;
}
.previewWeb .previewWeb_main .info .name {
  text-align: center;
  line-height: 51rpx;
  font-size: 30rpx;
  color: rgba(0, 0, 0, 0.6);
  margin-bottom: 8rpx;
}
.previewWeb .previewWeb_main .info .size {
  text-align: center;
  line-height: 28rpx;
  color: rgba(0, 0, 0, 0.3);
  font-size: 26rpx;
}
.previewWeb .footer_but {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-justify-content: space-around;
          justify-content: space-around;
  height: 100rpx;
  border-top: 1rpx solid #DDDDDD;
  background: #fff;
}
.previewWeb .footer_but .share, .previewWeb .footer_but .collect, .previewWeb .footer_but .information {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  height: 100rpx;
}
.previewWeb .footer_but .share .img, .previewWeb .footer_but .collect .img, .previewWeb .footer_but .information .img {
  width: 34rpx;
  height: 32rpx;
  display: block;
  margin-right: 15rpx;
}
.previewWeb .footer_but .share .txt, .previewWeb .footer_but .collect .txt, .previewWeb .footer_but .information .txt {
  font-size: 28rpx;
  color: rgba(0, 0, 0, 0.45);
}
.previewWeb .footer_but .share {
  padding: 0;
  background: #fff;
  margin: 0;
  height: 98rpx;
}
.previewWeb .footer_but .information {
  position: relative;
}
.previewWeb .footer_but .information .supernatant {
  background: #A4B3FF;
  position: absolute;
  left: -12rpx;
  bottom: 85rpx;
  width: 393rpx;
  padding: 30rpx 52rpx 36rpx 36rpx;
  line-height: 40rpx;
  color: white;
  font-size: 26rpx;
  border-radius: 6rpx;
  z-index: 1;
}
.previewWeb .footer_but .information .supernatant .triangleDiv {
  position: absolute;
  bottom: -10rpx;
  left: 20rpx;
  border-color: #A4B3FF transparent transparent transparent;
  border-style: solid;
  border-width: 12rpx 10rpx 0 10rpx;
  height: 0;
  width: 0;
}
.load {
  position: relative;
  width: 160rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin: 0 auto;
  margin-top: 62rpx;
  height: 36rpx;
}
.load .load_icon {
  position: absolute;
  left: 0;
  top: 0;
  bottom: 0;
  width: 36rpx;
  height: 36rpx;
  display: block;
  -webkit-animation: myfirst1 1s linear infinite;
          animation: myfirst1 1s linear infinite;
}
.load > .txt {
  padding-left: 50rpx;
  line-height: 36rpx;
  font-size: 24rpx;
  color: rgba(0, 0, 0, 0.25);
}
.zipShow {
  text-align: center;
  margin-top: 100rpx;
  line-height: 36rpx;
  font-size: 24rpx;
  color: rgba(0, 0, 0, 0.25);
}
.again {
  width: 510rpx;
  height: 82rpx;
  margin: 0 auto;
  background: #2F7AF5;
  margin-top: 100rpx;
  color: #fff;
  border-radius: 6rpx;
  font-size: 32rpx;
}
.searchFile1 {
  background: #fff;
}
.searchFile1 .searchFile_main .search_top {
  background: #EFEFF4;
}
@-webkit-keyframes myfirst1 {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
50% {
    -webkit-transform: rotate(180deg);
            transform: rotate(180deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@keyframes myfirst1 {
0% {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
50% {
    -webkit-transform: rotate(180deg);
            transform: rotate(180deg);
}
100% {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
.haibao {
  position: absolute;
  top: 800rpx;
  left: 0rpx;
  right: 0;
  bottom: 100rpx;
  margin: auto;
  z-index: 0;
  opacity: 0;
}
.haibao #code-canvas {
  margin: 0 auto;
  opacity: 0;
}

