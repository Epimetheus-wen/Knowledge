      margin-right: 3rpx;
      margin-left: -10rpx;
      .uni-icons {
        font-size: 38rpx !important;
        color: #000000 !important;
      }
    }
    .arrowleft_ba{
        position:absolute;
        left: 0;
        width: 60rpx;
        height: 80rpx;
    }
    .titleText {
      height: 80rpx;
      color: #000000;
      display: flex;
      align-items: center;
      font-size: 32rpx;
