  .geduan {
    height: calc(var(--status-bar-height) + 80rpx);
  }

  .titles {
    height: 80rpx;
    padding-left: 30rpx;
    display: flex;
    align-items: center;
    position: relative;
    .arrowleft {
      font-size: 22rpx;
