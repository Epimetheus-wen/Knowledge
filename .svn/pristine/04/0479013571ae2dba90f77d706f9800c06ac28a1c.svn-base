<template>
  <view class="header_top">
    <view class="status_bar" :style="'height: calc('+((staHeight*2)+80)+'rpx);'">
      <view class="top_view" :style="'height:'+staHeight+'px;'"></view>
      <view class="top titles">
        <uni-icons v-if="back" @tap="getBack" type="arrowleft" class="arrowleft"></uni-icons>
        <view v-if="back" class="arrowleft_ba" @tap="getBack"></view>
        <view class="titleText">{{titleTop}}</view>
      </view>
    </view>
    <view class="geduan" :style="'height: calc('+((staHeight*2)+80)+'rpx);'"></view>
  </view>
</template>

<script>
import uniIcons from "@dcloudio/uni-ui/lib/uni-icons/uni-icons.vue";
export default {
  props: ["titleTop"],
  data() {
    return {
      staHeight: "",
      back: false,
      noBack:false
    };
  },
  components: {
    uniIcons
  },
  created() {
    let routes = getCurrentPages();
    let curRoute = routes[routes.length - 1].route;
    console.log(curRoute);
    let that=this
    this.noBack=false
    if (curRoute == "pages/search/searchResult") {
      this.back = true;
    }
    if (curRoute == "pages/search/searchList") {
      this.back = true;
    }
    if (curRoute == "pages/classify/classifyList") {
      this.back = true;
    }
    if (curRoute == "pages/user/collectList") {
      this.back = true;
    }
    if (curRoute == "pages/preview/previewWeb") {
      this.back = true;
        var pages = getCurrentPages();
        console.log('pages===历史路由',pages)
        wx.getStorage({
            key: "token",
            success(res) {
                if(res.data){
                    that.noBack=true
                    if(pages.length<=1){
                        that.noBack=true
                    }else{
                        that.noBack=false
                    }
                }else{
                    that.noBack=false
                }
            },
            fail(res) {
                that.noBack=false
            }
        })
    }
    if (curRoute == "pages/preview/preview") {
        this.back = true;
        // wx.getStorage({
        //     key: "loginType",
        //     success(res) {
        //         that.loginType=true
        //         console.log(res.data)
        //         if(res.data==1007){
        //             that.noBack=true
        //         }else{
        //             that.noBack=false
        //         }
        //     },
        // })
        var pages = getCurrentPages();
        console.log('pages===历史路由',pages)
        wx.getStorage({
            key: "token",
            success(res) {
                if(res.data){
                    that.noBack=true
                    that.back = true;
                    if(pages.length<=1){
                        that.noBack=true
                    }else{
                        that.noBack=false
                    }
                }else{
                    that.noBack=false
                    that.back = false;
                }
            },
            fail(res) {
                that.noBack=false
                that.back = false;
            }
        })
    }
    if (curRoute == "pagesA/dataModule/dataModule") {
      this.back = true;
      that.noBack=true
    }
    if (curRoute == "pagesA/operationalAnalysis/operationalAnalysis") {
      this.back = true;
      that.noBack=true
    }
    if (curRoute == "pagesA/details/details") {
      this.back = true;
    }
    if (curRoute == "pagesA/oaPanel/oaPanel") {
      this.back = true;
    }
    if (curRoute == "pagesA/preList/preList") {
      this.back = true;
    }
    console.log("状态栏高度");
    wx.getSystemInfo({
      success: e => {
        console.log("状态栏高度", e.statusBarHeight);
        that.staHeight = e.statusBarHeight;
      }
    });
  },
  methods: {
    getBack() {
        console.log("this.noBack",this.noBack)
        if(this.noBack){

            uni.switchTab({
                url: '/pages/index/index'
            });
        }else{
            uni.navigateBack({
                delta: 1
            });
        }
      
    }
  }
};
</script>


