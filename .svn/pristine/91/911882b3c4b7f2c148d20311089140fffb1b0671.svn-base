ass="cal_data"><view class="time_txt"><view class="m1"><view class="title">开始：</view><view class="t1">{{customDate.data.start}}</view></view><view class="m1"><view class="title">结束：</view><view class="t1">{{customDate.data.end}}</view></view></view><button data-event-opts="{{[['tap',[['confirmTime',['$event']]]]]}}" class="time" bindtap="__e">确定</button></view></view></view><view class="details_Table_list"><view class="title"><view class="title_item title_item1">客户名称</view><view class="title_item">营收（万）</view><view class="title_item">成本（万）</view><view class="title_item">毛利（万）</view></view><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="list"><view class="list_item list_item1">{{item.business_type}}</view><view class="list_item">{{item.revenue}}</view><view class="list_item">{{item.cost}}</view><view class="list_item">{{item.profit}}</view></view></block><view class="list_total"><view class="list_item list_item1 list_item1_total">总计</view><view class="list_item">{{total.revenue}}</view><view class="list_item">{{total.cost}}</view><view class="list_item">{{total.profit}}</view></view></view></view></view></view>