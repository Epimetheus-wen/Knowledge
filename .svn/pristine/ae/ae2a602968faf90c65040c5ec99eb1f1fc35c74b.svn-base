.previewWeb .previewWeb_main {
  min-height: calc(100vh - (25px + 80rpx));
  position: relative;
}
.previewWeb .previewWeb_main .previewWeb_main_top {
  padding-top: 9rpx;
  position: relative;
}
.previewWeb .previewWeb_main .previewWeb_main_top .top_title {
  margin-left: 30rpx;
  line-height: 51rpx;
  color: rgba(0, 0, 0, 0.85);
  font-size: 30rpx;
}
.previewWeb .previewWeb_main .previewWeb_main_top .top_item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin: 0 30rpx;
  padding: 7rpx 0 8rpx 0;
  border-bottom: 2rpx solid rgba(0, 0, 0, 0.1);
}
.previewWeb .previewWeb_main .previewWeb_main_top .top_item .top_item_bq {
  width: 106rpx;
  height: 38rpx;
  background: #c1cadc;
  border-radius: 19rpx;
  color: #fff;
  font-size: 22rpx;
  line-height: 38rpx;
  text-align: center;
  margin-right: 13rpx;
}
.previewWeb .previewWeb_main .previewWeb_main_top .top_item .top_item_time {
  font-size: 20rpx;
  color: rgba(0, 0, 0, 0.3);
  line-height: 51rpx;
}
.previewWeb .previewWeb_main .previewWeb_main_top .top_item_but {
  position: absolute;
  right: 0;
  bottom: 24rpx;
  height: 40rpx;
  background-image: url(data:image/png;base64,);
  background-size: 100% 100%;
  padding: 0 46rpx 0 24rpx;
}
.previewWeb .previewWeb_main .previewWeb_main_top .top_item_but .img {
  width: 100%;
  height: 100%;
  display: block;
}
.previewWeb .previewWeb_main .previewWeb_main_top .top_item_but .txt {
  font-size: 24rpx;
  color: #FFFFFF;
  line-height: 40rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.previewWeb .previewWeb_main .previewWeb_main_top .top_item_but .txt text {
  max-width: 130rpx;
  color: #FFD200;
  display: block;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.previewWeb .previewWeb_main .previewWeb_main_center {
  margin: 44rpx 30rpx 0rpx 30rpx;
  padding-bottom: 313rpx;
}
.previewWeb .previewWeb_main .previewWeb_main_center .center_main {
  font-size: 26rpx;
  color: rgba(0, 0, 0, 0.65);
  line-height: 40rpx;
}
.previewWeb .previewWeb_main .previewWeb_main_center .center_main .imgs {
  margin: 20rpx auto;
  display: block;
  width: 100%;
  height: 100%;
}
.previewWeb .previewWeb_main .previewWeb_main_bottom {
  position: absolute;
  bottom: 168rpx;
  left: 36rpx;
}
.previewWeb .previewWeb_main .previewWeb_main_bottom .item {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  font-size: 22rpx;
  color: rgba(0, 0, 0, 0.45);
  line-height: 38rpx;
}
.previewWeb .footer_but {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: end;
  -webkit-justify-content: flex-end;
          justify-content: flex-end;
  height: 100rpx;
  border-top: 1rpx solid #DDDDDD;
  background: #fff;
}
.previewWeb .footer_but .share, .previewWeb .footer_but .collect {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  margin-right: 49rpx;
}
.previewWeb .footer_but .share .img, .previewWeb .footer_but .collect .img {
  width: 34rpx;
  height: 32rpx;
  display: block;
  margin-right: 15rpx;
}
.previewWeb .footer_but .share .txt, .previewWeb .footer_but .collect .txt {
  font-size: 28rpx;
  color: rgba(0, 0, 0, 0.45);
}
.previewWeb .footer_but .share {
  padding: 0;
  background: #fff;
}
