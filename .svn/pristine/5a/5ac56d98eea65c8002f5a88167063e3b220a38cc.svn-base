<view data-event-opts="{{[['tap',[['dowInfo',['$event']]]]]}}" class="previewWeb" bindtap="__e"><headertop vue-id="c603748c-1" titleTop="{{titleTop}}" bind:__l="__l"></headertop><view class="previewWeb_main" style="{{('min-height: calc(100vh - ('+(staHeight*2+80)+'rpx) - 190rpx);')}}"><view class="previewWeb_main_center"><block wx:if="{{obj.type==1&&(obj.accessory_type=='docx'||obj.accessory_type=='doc')}}"><image class="img" mode="widthFix" src="../../static/home/word.png"></image></block><block wx:if="{{obj.type==1&&(obj.accessory_type=='ppt'||obj.accessory_type=='pptx')}}"><image class="img" mode="widthFix" src="../../static/home/ppt.png"></image></block><block wx:if="{{obj.type==1&&obj.accessory_type=='pdf'}}"><image class="img" mode="widthFix" src="../../static/home/pdf.png"></image></block><block wx:if="{{obj.type==1&&(obj.accessory_type=='xls'||obj.accessory_type=='xlsx')}}"><image class="img" mode="widthFix" src="../../static/home/excel.png"></image></block><block wx:if="{{obj.type==1&&(obj.accessory_type=='rar'||obj.accessory_type=='zip')}}"><image class="img" mode="widthFix" src="../../static/home/zip.png"></image></block><block wx:if="{{obj.type==1&&(obj.accessory_type=='png'||obj.accessory_type=='jpg')}}"><image class="img" mode="widthFix" src="../../static/home/img.png"></image></block><block wx:if="{{obj.type==1&&obj.accessory_type=='txt'}}"><image class="img" mode="widthFix" src="../../static/home/txt.png"></image></block></view><view class="info"><view class="name">{{obj.accessory_name}}</view><view class="size">{{'文件大小：'+obj.accessory_size+'M'}}</view><block wx:if="{{againShow}}"><view class="load"><image class="load_icon" src="../../static/user/load_03.png"></image><view class="txt">正在加载</view></view></block><block wx:else><button data-event-opts="{{[['tap',[['downloadFile',['$0'],['obj.filePath']]]]]}}" class="again" style="{{(zipShow?'display:none;':'display:block;')}}" bindtap="__e">{{againTxt}}</button></block><block wx:if="{{zipShow}}"><view class="zipShow">请前往PC端操作</view></block></view></view></view>