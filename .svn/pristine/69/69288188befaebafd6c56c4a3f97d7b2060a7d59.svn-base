.search {
  letter-spacing: 1rpx;
  height: 100vh;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.search .search_main {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
          flex: 1;
  background: #EFEFF4;
}
.search .search_main .search_top {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  height: 92rpx;
}
.search .search_main .search_top .search_top_left {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  width: 68rpx;
  height: 88rpx;
}
.search .search_main .search_top .search_top_left > .img {
  width: 14rpx;
  height: 26rpx;
  display: block;
  margin-left: 10rpx;
}
.search .search_main .search_top .search_top_center {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  position: relative;
  width: 570rpx;
  height: 60rpx;
  background: white;
  box-shadow: 0px 2rpx 10rpx 0px #d5d5d5;
  border-radius: 6rpx;
}
.search .search_main .search_top .search_top_center > .input {
  width: 100%;
  height: 100%;
  padding-left: 66rpx;
  padding-right: 54rpx;
  font-size: 26rpx;
  position: relative;
  z-index: 1;
}
.search .search_main .search_top .search_top_center .input_s {
  font-size: 26rpx;
  color: #BBBBBB;
}
.search .search_main .search_top .search_top_center .img {
  width: 30rpx;
  height: 30rpx;
  display: block;
  z-index: 2;
}
.search .search_main .search_top .search_top_center > .img1 {
  position: absolute;
  left: 24rpx;
  top: 0;
  bottom: 0;
  margin: auto 0;
}
.search .search_main .search_top .search_top_center > .img2 {
  position: absolute;
  padding: 0 24rpx;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto 0;
}
.search .search_main .search_top .search_top_right {
  width: 112rpx;
  font-size: 28rpx;
  color: #2F7AF5;
  text-align: center;
}
.search .search_main .search_center {
  margin: 27rpx 69rpx 0 69rpx;
}
.search .search_main .search_center .search_center_title {
  font-size: 26rpx;
  color: black;
  line-height: 51rpx;
}
.search .search_main .search_center .search_center_main {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
}
.search .search_main .search_center .search_center_main > .txt {
  font-size: 24rpx;
  color: rgba(0, 0, 0, 0.3);
  line-height: 28rpx;
  margin-top: 15rpx;
}
.search .search_main .search_center .search_center_main > .search_jg {
  padding: 12rpx 24rpx;
  margin: 20rpx 20rpx 0 0;
  background: white;
  box-shadow: 0px 2rpx 6rpx 0px rgba(213, 213, 213, 0.8);
  border-radius: 25rpx;
  display: inline-block;
  color: #2f7af5;
  line-height: 26rpx;
  font-size: 24rpx;
}

